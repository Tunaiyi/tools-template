							<demand>
								<itemAlias>DYN_WORLD$world</itemAlias>
								<name>world</name>
								<demandType>OFFICE_DYNASTY_WAR_TIME_LIMIT</demandType>
								<fx><![CDATA[!world.isWaring()]]></fx>
							</demand>
						</demandList>
						<costPlan>
							<costList>
								<cost>
									<itemAlias>RESC$gold</itemAlias>
									<expect><![CDATA[0]]></expect>
								</cost>
							</costList>
						</costPlan>
					</actionPlan>
				</actionPlanList>
			</behavioPlan>
			<behavioPlan>
				<behavior>OFFICER_$VENERATE</behavior>
				<actionPlanList>
					<actionPlan>
						<action>OFFICER_$VENERATE_$NORMAL</action>
						<awardPlan>
							<awardGroupSet>
								<awardGroup>
									<awardList>
										<award>
											<itemAlias>RESC$coin</itemAlias>
											<fx><![CDATA[player.getLevel()*player.getLevel()*30]]></fx>
										</award>
									</awardList>
								</awardGroup>
							</awardGroupSet>
						</awardPlan>
					</actionPlan>
				</actionPlanList>
			</behavioPlan>
		</behaviorPlanList>		
